-endif ()
 
 add_dependencies(Sourcetrail Sourcetrail_indexer)
 
@@ -84,4 +85,4 @@ file(COPY ${CMAKE_SOURCE_DIR}/bin/app/data/gui/icon/sourcetrail.ico
 
 if(WIN32)
   copy_qt_binaries("${CMAKE_BINARY_DIR}/app/" TRUE)
-endif()
\ No newline at end of file
